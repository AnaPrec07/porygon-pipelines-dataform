FROM ${ref("target_store_item_monthly")} tgt

-- join features
LEFT JOIN ${ref("calendar_features")} cal
    ON cal.ctx_date_month = tgt.ctx_date_month
LEFT JOIN ${ref("price_features_ts")} price_features
    ON price_features.ctx_date_month = tgt.ctx_date_month
    AND price_features.ctx_item_id = tgt.ctx_item_id
    AND price_features.ctx_store_id = tgt.ctx_store_id
LEFT JOIN ${ref("encoded_features")} enc
    ON enc.ctx_date_month = tgt.ctx_date_month
    AND enc.ctx_item_id = tgt.ctx_item_id
    AND enc.ctx_store_id = tgt.ctx_store_id
LEFT JOIN ${ref("weather_features")} wea_feas
    ON wea_feas.ctx_state_id = tgt.ctx_state_id
    AND wea_feas.ctx_date_month = tgt.ctx_date_month
LEFT JOIN ${ref("population_features")} pop_feas
    ON pop_feas.ctx_state_id = tgt.ctx_state_id
    AND ctx_date_year = EXTRACT(YEAR FROM tgt.ctx_date_month)
LEFT JOIN ${ref("economic_indicator_features")} econ_feas
    ON econ_feas.ctx_date_month = tgt.ctx_date_month
LEFT JOIN ${ref("rolling_percent_sales")} percent_feas
    ON percent_feas.ctx_date_month = tgt.ctx_date_month
LEFT JOIN ${ref("rolling_sales_features")} roll_feas
    ON roll_feas.ctx_date_month = tgt.ctx_date_month
    AND roll_feas.ctx_item_id = tgt.ctx_item_id
    AND roll_feas.ctx_store_id = tgt.ctx_store_id
LEFT JOIN ${ref("global_rolling_sales_features")} glob_feas
    ON glob_feas.ctx_date_month = tgt.ctx_date_month
LEFT JOIN ${ref("autocorrelation_features")} aucorr_feas
    ON aucorr_feas.ctx_date_month = tgt.ctx_date_month
    AND aucorr_feas.ctx_item_id = tgt.ctx_item_id
    AND aucorr_feas.ctx_store_id = tgt.ctx_store_id
LEFT JOIN ${ref("dept_sales_correlations")} deptcorr_feas
    ON deptcorr_feas.ctx_date_month = tgt.ctx_date_month
    AND deptcorr_feas.ctx_item_id = tgt.ctx_item_id
    AND deptcorr_feas.ctx_store_id = tgt.ctx_store_id
LEFT JOIN ${ref("historical_stockout_features")} histckt_feas
    ON histckt_feas.ctx_date_month = tgt.ctx_date_month
    AND histckt_feas.ctx_item_id = tgt.ctx_item_id
    AND histckt_feas.ctx_store_id = tgt.ctx_store_id
LEFT JOIN ${ref("item_longevity_features")} long_feas
    ON long_feas.ctx_date_month = tgt.ctx_date_month
    AND long_feas.ctx_item_id = tgt.ctx_item_id
LEFT JOIN ${ref("seasonality_correlation")} sea_feas
    ON sea_feas.ctx_date_month = tgt.ctx_date_month
    AND sea_feas.ctx_item_id = tgt.ctx_item_id
    AND sea_feas.ctx_store_id = tgt.ctx_store_id
LEFT JOIN ${ref("trend_correlation_features")} trend_feas
    ON trend_feas.ctx_date_month = tgt.ctx_date_month
    AND trend_feas.ctx_item_id = tgt.ctx_item_id
    AND trend_feas.ctx_store_id = tgt.ctx_store_id

-- join filter_out_outliers
LEFT JOIN ${ref("stockout_filter")} stk
    ON stk.ctx_date_month = tgt.ctx_date_month
    AND stk.ctx_item_id = tgt.ctx_item_id
    AND stk.ctx_store_id = tgt.ctx_store_id
LEFT JOIN ${ref("target_outliers_filter")} tf
    ON tf.ctx_date_month = tgt.ctx_date_month
    AND tf.ctx_item_id = tgt.ctx_item_id
    AND tf.ctx_store_id = tgt.ctx_store_id

WHERE tgt.ctx_store_id = "CA_1"